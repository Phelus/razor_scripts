endif
if hits < 75
    if skill 'Healing' > 30 and timer 'bands' >= 11000 and findtype 3617 backpack and timer 'tba' >= 600
        usetype 3617
        waitfortarget 5000
        target 'self'
        settimer 'bands' 0
        settimer 'tba' 0
    elseif skill 'Magery' > 30 and timer 'cast_recovery' >= 2000 and mana >= 14 and count 'garlic' > 0 and count 'ginseng' > 0 and count 'mandrake root' > 0 and count 'spiders silk' > 0 and timer 'tba' >= 600
        target cancel
        cast 'greater heal'
        waitfortarget 5000
        target 'self'
        settimer 'cast_recovery' 0
        settimer 'tba' 0
    elseif count 'heal potions' and timer 'heal_pot' >= 11000 and timer 'tba' >= 600
        potion 'heal'
        settimer 'heal_pot' 0
        settimer 'tba' 0
    endif
endif

if poisoned and count 'cure potions' and timer 'cure_pot' >= 11000 and timer 'tba' >= 600
    potion 'cure'
    settimer 'cure_pot' 0
    settimer 'tba' 0
endif

